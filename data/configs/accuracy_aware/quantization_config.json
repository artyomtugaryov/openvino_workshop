{
    "model": {
        "model_name": "ssd_mobilenet_v2_coco",
        "model": "data/public/ssd_mobilenet_v2_coco/FP32/ssd_mobilenet_v2_coco.xml",
        "weights": "data/public/ssd_mobilenet_v2_coco/FP32/ssd_mobilenet_v2_coco.bin"
    },
    "engine": {
        "launchers": [
            {
                "framework": "dlsdk",
                "device": "CPU",
                "adapter": {
                    "type": "ssd"
                },
                "batch": 1
            }
        ],
        "datasets": [
            {
                "name": "dataset",
                "data_source": "data/datasets/COCO2017/val2017",
                "annotation_conversion": {
                    "annotation_file": "data/datasets/COCO2017/annotations/instances_val2017.json",
                    "has_background": true,
                    "use_full_label_map": true,
                    "converter": "mscoco_detection",
                    "images_dir": "data/datasets/COCO2017/val2017"
                },
                "subsample_size": "100%",
                "preprocessing": [
                    {
                        "type": "resize",
                        "size": 300
                    }
                ],
                "postprocessing": [
                    {
                        "type": "resize_prediction_boxes"
                    }
                ],
                "metrics": [
                    {
                        "type": "coco_precision",
                        "presenter": "print_vector"
                    }
                ]
            }
        ]
    },
    "compression": {
        "target_device": "CPU",
        "algorithms": [
            {
                "name": "AccuracyAwareQuantization",
                "params": {
                    "maximal_drop": 0.01,
                    "metric_subset_ratio": 1,
                    "base_algorithm": "DefaultQuantization",
                    "ranking_subset_size": 200,
                    "stat_subset_size": 200
                }
            }
        ]
    }
}